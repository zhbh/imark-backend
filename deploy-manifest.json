{
    "version": 1,
    "framework": {
        "name": "express",
        "version": "4.21.0"
    },
    "imageSettings": {
        "sizes": [
            100,
            200,
            1920
        ],
        "domains": [],
        "remotePatterns": [],
        "formats": [],
        "minimumCacheTTL": 60,
        "dangerouslyAllowSVG": false
    },
    "routes": [
        {
            "path": "/_amplify/image",
            "target": {
                "kind": "ImageOptimization",
                "cacheControl": "public, max-age=3600, immutable"
            }
        },
        {
            "path": "/*.*",
            "target": {
                "kind": "Static",
                "cacheControl": "public, max-age=2"
            },
            "fallback": {
                "kind": "Compute",
                "src": "default"
            }
        },
        {
            "path": "/*",
            "target": {
                "kind": "Compute",
                "src": "default"
            }
        }
    ],
    "computeResources": [
        {
            "name": "default",
            "runtime": "nodejs20.x",
            "entrypoint": "index.js"
        }
    ]
}